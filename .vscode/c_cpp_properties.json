{
  "version": 4,
  "configurations": [
    {
      "name": "NeverMind-WSL",
      "includePath": [
        "${workspaceFolder}/include",
        "${workspaceFolder}"
      ],
      "defines": [
        "__x86_64__",
        "__KERNEL__"
      ],
      "compilerPath": "/usr/bin/gcc",
      "cStandard": "c11",
      "cppStandard": "c++17",
      "intelliSenseMode": "linux-gcc-x64"
    },
    {
      "name": "NeverMind-Linux",
      "includePath": [
        "${workspaceFolder}/include",
        "${workspaceFolder}"
      ],
      "defines": [
        "__x86_64__",
        "__KERNEL__"
      ],
      "compilerPath": "/usr/bin/clang",
      "cStandard": "c11",
      "cppStandard": "c++17",
      "intelliSenseMode": "linux-clang-x64"
    },
    {
      "name": "NeverMind-Windows-Host",
      "includePath": [
        "${workspaceFolder}/include",
        "${workspaceFolder}"
      ],
      "defines": [
        "__x86_64__",
        "__KERNEL__"
      ],
      "cStandard": "c11",
      "cppStandard": "c++17",
      "intelliSenseMode": "windows-gcc-x64"
    }
  ]
}
